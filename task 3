#include <cs50.h>
#include <stdio.h>

int main(void)
{
    int m;
    printf("Введіть номер місяця: ");
    scanf("%i", &m);
    if (m==4 || m==6 || m==9 || m==11)
    {
        printf("30 днів\n");
    }
    else if ( m==2)
    {
        printf("28 днів\n");
    }
    else
    {
        printf("31 день\n");
    }
}
